pipeline {
    
    agent any
     tools {
      terraform 'Terraform v1.5.1'
      git 'Default'
	  dockerTool 'Docker'
    }
    stages {
       stage(aws_version_check) {
           steps {
               
               sh 'aws --version '
               sh 'ls /var/log/'
               sh 'echo "aws version has been printed"'
               sh 'whoami'
           }
       }
       stage(terraform_version_check) {
           steps {
               
               sh 'terraform --version'
               sh 'echo "terraform version has been printed"'
	       sh 'ls'
               sh 'cd Terraform'
               sh 'terraform init'
      
                         
           }           
           
       }
       
       
    }
}
